{
  "name": "quantum-basic",
  "version": "1.0.0",
  "description": "BASIC interpreter with quantum computing capabilities",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node --watch backend/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm install && npm run build",
    "start": "node backend/server.js"
  },
  "keywords": ["basic", "quantum", "interpreter", "kiroween"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
